{
  "name": "forkcast",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://wolovim.github.io/forkcast",
  "scripts": {
    "compile-eips": "node scripts/compile-eips.mjs",
    "dev": "npm run compile-eips && vite",
    "build": "npm run compile-eips && tsc -b && vite build && node scripts/generate-static-pages.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "check-reviewers": "node scripts/eips-by-reviewer.mjs",
    "generate-configs": "node scripts/generate-configs.cjs",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "react-youtube": "^10.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "pre-commit": "^1.2.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "pre-commit": [
    "format"
  ]
}
